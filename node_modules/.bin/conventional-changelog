#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/conventional-changelog@7.1.0_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/dist/cli/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/conventional-changelog@7.1.0_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/dist/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/conventional-changelog@7.1.0_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/conventional-changelog@7.1.0_conventional-commits-filter@5.0.0/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/conventional-changelog@7.1.0_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/dist/cli/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/conventional-changelog@7.1.0_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/dist/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/conventional-changelog@7.1.0_conventional-commits-filter@5.0.0/node_modules/conventional-changelog/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/conventional-changelog@7.1.0_conventional-commits-filter@5.0.0/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../conventional-changelog/dist/cli/index.js" "$@"
else
  exec node  "$basedir/../conventional-changelog/dist/cli/index.js" "$@"
fi
