#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/mkdist@2.3.0_typescript@5.8.3_vue@3.5.17_typescript@5.8.3_/node_modules/mkdist/dist/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/mkdist@2.3.0_typescript@5.8.3_vue@3.5.17_typescript@5.8.3_/node_modules/mkdist/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/mkdist@2.3.0_typescript@5.8.3_vue@3.5.17_typescript@5.8.3_/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/mkdist@2.3.0_typescript@5.8.3_vue@3.5.17_typescript@5.8.3_/node_modules/mkdist/dist/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/mkdist@2.3.0_typescript@5.8.3_vue@3.5.17_typescript@5.8.3_/node_modules/mkdist/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/mkdist@2.3.0_typescript@5.8.3_vue@3.5.17_typescript@5.8.3_/node_modules:/Users/angck/open-source/universe-vue-template/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../dist/cli.cjs" "$@"
else
  exec node  "$basedir/../../dist/cli.cjs" "$@"
fi
